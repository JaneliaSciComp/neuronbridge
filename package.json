  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "sls export-env && HTTPS=true SSL_CRT_FILE=./cert/server.cert SSL_CRT_KEY=./cert/server.key node scripts/start.js",
    "start:devpre": "sls export-env --stage devpre && REACT_APP_NO_OKTA=true REACT_APP_LEVEL=devpre HTTPS=true SSL_CRT_FILE=./cert/server.cert SSL_CRT_KEY=./cert/server.key node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "node scripts/test.js",
    "test:integration": "cypress run",
    "test:devel": "cypress open",
    "deploy:dev": "npm run test && REACT_APP_LEVEL=dev sls client build && sls client deploy --no-confirm --stage dev",
    "deploy:val": "npm run test && REACT_APP_LEVEL=val REACT_APP_DATA_TARGET=prod sls client build --stage val && sls client deploy --no-confirm --stage val",
    "deploy:prod": "npm run test && REACT_APP_ANALYTICS_KEY=MIHSEMHS REACT_APP_LEVEL=prod sls client build --stage prod && sls client deploy --no-comfirm --stage prod",
    "deploy:devpre": "npm run test && REACT_APP_LEVEL=devpre REACT_APP_NO_OKTA=true sls client build --stage devpre && sls client deploy --no-confirm --stage devpre",
    "deploy:prodpre": "npm run test && REACT_APP_ANALYTICS_KEY=ERAVKYRB REACT_APP_LEVEL=prodpre sls client build --stage prodpre && sls client deploy --no-confirm --stage prodpre",
    "storybook": "start-storybook  -s ./public -p 6007",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.4.13",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/react": "^9.5.0",
    "babel-jest": "^29.0.3",
    "cypress": "^10.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^3.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-axe": "^3.4.0",
    "jest-environment-jsdom": "^29.0.3",
    "less-loader": "^6.0.0",
    "prettier": "^2.7.1",
    "react-is": "^16.13.1",
    "serverless-build-client": "^2.5.0",
    "serverless-deployment-bucket": "^1.5.1",
    "serverless-export-env": "^2.0.0",
    "serverless-finch": "^2.6.0",
    "serverless-plugin-package-json": "^0.1.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.jsx"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
